<div class="container">
  <a href="parkings/create" id="createParkings" class="">Create a new parking</a>
  <div class="row">
    {{#each parkdata}}
      <div class="col-sm-4">

        <div class="card">
          <img src={{parkingImg}} class="card-img-top" alt="Parking Image" />
          <div class="card-body">
            <h5 class="card-title">{{address}}</h5>
            <p class="card-text">{{zip}}</p>
            <a href="/{{id}}" class="btn btn-primary">Go Here</a>
            <a href="#" id="editParkings" class="btn btn-primary">Edit</a>
            <input
              type="hidden"
              id="parkingID"
              name="parkingID"
              value={{_id}}
            />
          </div>
        </div>
      </div>
    {{/each}}
  </div>
</div>